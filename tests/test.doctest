>>> import bloomfilter
>>> from hashlib import md5

>>> x = bloomfilter.BloomFilter()
>>> y = open('foo.txt').read()
>>> g = y.split()
>>> for y in g:
...     dig = md5()
...     dig.update(y.encode())
...     x.add(dig.hexdigest())
...
>>> allIn = False
>>> for y in g:
...     dig = md5()
...     dig.update(y.encode())
...     allIn = dig.hexdigest() in x
...     if not allIn:
...         break
...
>>> allIn
True

>>> dig = md5()
>>> dig.update(b'foobar')
>>> dig.hexdigest() in x
False
